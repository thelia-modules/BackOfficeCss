<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="backofficecss.show" path="/admin/module/BackOfficeCss" methods="get">
        <default key="_controller">BackOfficeCss:StylesheetEditor:showEditor</default>
    </route>
    <route id="backofficecss.save" path="/admin/module/BackOfficeCss" methods="post">
        <default key="_controller">BackOfficeCss:StylesheetEditor:save</default>
    </route>

</routes>
